    <script type="text/javascript">
        $(document).ready(function () {
            $.ajaxSetup({
                beforeSend: function (xhr, opts) {
                    if (opts.type.toLowerCase() === "post") {
                        xhr.setRequestHeader('RequestVerificationToken', '@Utility.TokenHeaderValue()');
                    }
                },
                complete: function (xhr, status) {
                    if (xhr.getResponseHeader("CustomHeader") == "RedirectToLogin" && xhr.status == 404) {
                        window.location.assign("/error" + "?errorMessage=Anti forgery token not found");
                    }
                }
            });
        });
